{
  "name": "fastapi",
  "dockerComposeFile": "../docker-compose.yaml",
  "service": "fastapi",
  "workspaceFolder": "/workspace/fastapi",
  "forwardPorts": [8000],
  "customizations": {
    "vscode": {
      "extensions": [
        "ms-python.python",
        "charliermarsh.ruff",
        "ms-vsliveshare.vsliveshare"
      ],
      "settings": {
        "[python]": {
          "editor.defaultFormatter": "charliermarsh.ruff",
          "editor.formatOnSave": true
        }
      }
    }
  },
  "overrideCommand": true
}